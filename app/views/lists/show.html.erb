<h1> <%= @list.name %></h1>
  <% @list.bookmarks.each do |bookmark| %>
  <%  %> 
  <h2><%= bookmark.movie.title %> </h2>  
  <p> <%= bookmark.comment %></p>
  <p><%= link_to "Delete", bookmark_path(bookmark), method: :delete, data: { confirm: "Are you sure you want to delete #{bookmark.movie.title}?"}%> </span></p>
  <% end %>
<%= link_to "Add a new movie to this list", new_list_bookmark_path(@list) %>
